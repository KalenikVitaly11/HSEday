<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_about_faculty"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hseday.AboutFacultyActivity"
    android:orientation="vertical">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:id="@+id/Faculty"/>

    <TextView
        android:text="@string/faculty_contacts_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/FacultyTitle" />

    <TextView
        android:text="TextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/FacultyInformation" />

    <TextView
        android:text="@string/departments"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/FacultyDepartments"/>

    <TextView
        android:text="TextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />
    <TextView
        android:text="@string/contancts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textStyle="normal|bold"/>
    <TextView
        android:text="TextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/FacultyContacts"/>
</LinearLayout>
